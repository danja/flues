@prefix atom: <http://lv2plug.in/ns/ext/atom#> .
@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix midi: <http://lv2plug.in/ns/ext/midi#> .
@prefix ui: <http://lv2plug.in/ns/extensions/ui#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<https://danja.github.io/flues/plugins/pm-synth>
    a lv2:InstrumentPlugin ;
    doap:name "Stove Synth" ;
    doap:description "Monophonic physical modelling synthesizer ported from the Flues browser experiment." ;
    doap:license <https://opensource.org/licenses/MIT> ;
    doap:maintainer [
        foaf:name "Danny Ayers" ;
        foaf:homepage <https://danja.github.io/flues/>
    ] ;
    lv2:requiredFeature <http://lv2plug.in/ns/ext/urid#map> ;
    ui:ui <https://danja.github.io/flues/plugins/pm-synth#ui> ;
    lv2:port [
        a lv2:OutputPort , lv2:AudioPort ;
        lv2:index 0 ;
        lv2:symbol "audio_out" ;
        lv2:name "Audio Out"
    ] , [
        a lv2:InputPort , atom:AtomPort ;
        lv2:index 1 ;
        lv2:symbol "midi_in" ;
        lv2:name "MIDI In" ;
        atom:bufferType atom:Sequence ;
        atom:supports midi:MidiEvent
    ] , [
        a lv2:InputPort , lv2:ControlPort ;
        lv2:index 2 ;
        lv2:symbol "dcLevel" ;
        lv2:name "DC Level" ;
        lv2:default 0.5 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.0
    ] , [
        a lv2:InputPort , lv2:ControlPort ;
        lv2:index 3 ;
        lv2:symbol "noiseLevel" ;
        lv2:name "Noise Level" ;
        lv2:default 0.15 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.0
    ] , [
        a lv2:InputPort , lv2:ControlPort ;
        lv2:index 4 ;
        lv2:symbol "toneLevel" ;
        lv2:name "Tone Level" ;
        lv2:default 0.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.0
    ] , [
        a lv2:InputPort , lv2:ControlPort ;
        lv2:index 5 ;
        lv2:symbol "attack" ;
        lv2:name "Attack" ;
        lv2:default 0.3333333333 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.0
    ] , [
        a lv2:InputPort , lv2:ControlPort ;
        lv2:index 6 ;
        lv2:symbol "release" ;
        lv2:name "Release" ;
        lv2:default 0.2821702825 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.0
    ] , [
        a lv2:InputPort , lv2:ControlPort ;
        lv2:index 7 ;
        lv2:symbol "interfaceType" ;
        lv2:name "Interface Type" ;
        lv2:default 2 ;
        lv2:minimum 0 ;
        lv2:maximum 11 ;
        lv2:portProperty lv2:integer , lv2:enumeration
    ] , [
        a lv2:InputPort , lv2:ControlPort ;
        lv2:index 8 ;
        lv2:symbol "interfaceIntensity" ;
        lv2:name "Interface Intensity" ;
        lv2:default 0.5 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.0
    ] , [
        a lv2:InputPort , lv2:ControlPort ;
        lv2:index 9 ;
        lv2:symbol "tuning" ;
        lv2:name "Tuning" ;
        lv2:default 0.5 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.0
    ] , [
        a lv2:InputPort , lv2:ControlPort ;
        lv2:index 10 ;
        lv2:symbol "ratio" ;
        lv2:name "Delay Ratio" ;
        lv2:default 0.5 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.0
    ] , [
        a lv2:InputPort , lv2:ControlPort ;
        lv2:index 11 ;
        lv2:symbol "delay1Feedback" ;
        lv2:name "Delay 1 Feedback" ;
        lv2:default 0.9595959596 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.0
    ] , [
        a lv2:InputPort , lv2:ControlPort ;
        lv2:index 12 ;
        lv2:symbol "delay2Feedback" ;
        lv2:name "Delay 2 Feedback" ;
        lv2:default 0.9595959596 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.0
    ] , [
        a lv2:InputPort , lv2:ControlPort ;
        lv2:index 13 ;
        lv2:symbol "filterFeedback" ;
        lv2:name "Filter Feedback" ;
        lv2:default 0.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.0
    ] , [
        a lv2:InputPort , lv2:ControlPort ;
        lv2:index 14 ;
        lv2:symbol "filterFrequency" ;
        lv2:name "Filter Frequency" ;
        lv2:default 0.5663233348 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.0
    ] , [
        a lv2:InputPort , lv2:ControlPort ;
        lv2:index 15 ;
        lv2:symbol "filterQ" ;
        lv2:name "Filter Q" ;
        lv2:default 0.1879018247 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.0
    ] , [
        a lv2:InputPort , lv2:ControlPort ;
        lv2:index 16 ;
        lv2:symbol "filterShape" ;
        lv2:name "Filter Shape" ;
        lv2:default 0.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.0
    ] , [
        a lv2:InputPort , lv2:ControlPort ;
        lv2:index 17 ;
        lv2:symbol "lfoFrequency" ;
        lv2:name "LFO Frequency" ;
        lv2:default 0.7383519587 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.0
    ] , [
        a lv2:InputPort , lv2:ControlPort ;
        lv2:index 18 ;
        lv2:symbol "modulationTypeLevel" ;
        lv2:name "Modulation Type Level" ;
        lv2:default 0.5 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.0
    ] , [
        a lv2:InputPort , lv2:ControlPort ;
        lv2:index 19 ;
        lv2:symbol "reverbSize" ;
        lv2:name "Reverb Size" ;
        lv2:default 0.5 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.0
    ] , [
        a lv2:InputPort , lv2:ControlPort ;
        lv2:index 20 ;
        lv2:symbol "reverbLevel" ;
        lv2:name "Reverb Level" ;
        lv2:default 0.3 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.0
    ] .

<https://danja.github.io/flues/plugins/pm-synth#ui>
    a ui:Gtk3UI ;
    ui:binary <pm_synth_ui.so> ;
    rdfs:label "Flues PM Synth Panel" .
